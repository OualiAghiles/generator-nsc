{
  "name": "<%= appname %>",
  "version": "<%= appversion %>",
  "authors": [
    "<%= appauthor %> <<%= appemail %>>"
  ],
  "moduleType": [
    "globales"
  ],
  "license": "<%= applicense %>",
  "ignore": [
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "devDependencies": {
    <% if (includeJQuery){ %>
    "jquery": "^3.1.1",
    <% } %>
    <% if (includeBootstrap){ %>
    "bootstrap-sass": "^3.3.7",
    <% } %>
    <% if (includeBourbon){ %>
    "bourbon": "^4.2.7",
    <% } %>
    <% if (includeNeat){ %>
    "neat": "^1.8.0",
    <% } %>
  }
}
