{
  "name": "starting",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "gulp --env debug",
    "build": "gulp --env production && gulp rev"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    <% if (includeBootstrap) { %>
    "bootstrap": "^4.1.3",
    <% } %>
    <% if (includeBootstrap || includeJQuery) { %>
    "jquery": "^3.3.1"<% if (includeModernizr || includeBootstrap) { %>,<% } %>
    <% } %>
    <% if (includeModernizr) { %>
    "modernizr": "^3.6.0"<% if (includeBootstrap) { %>,<% } %>
    <% } %>
    <% if (includeBootstrap) { %>
    "popper.js": "^1.14.6"
    <% } %>
  },
  "devDependencies": {

    "gulp-if": "^2.0.2",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-latest": "^6.16.0",
    "browser-sync": "^2.18.2",
    "eslint": "^3.10.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-changed": "^1.3.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-plumber": "^1.1.0",
    <% if (includePug) { %>
    "gulp-pug": "^3.1.0",
    <% } %>
    "gulp-replace": "^0.5.4",
    "gulp-rev": "^7.1.2",
    <% if (includeSass) { %>
    "gulp-sass": "^2.3.2",
    <%} %>
    "gulp-size": "^2.1.0",
    "gulp-util": "^3.0.7",
    "rimraf-promise": "^2.0.0",
    "run-sequence": "^1.2.2",
    "webpack": "^1.13.3"
  },
  "scripts": {
    <% if (includeModernizr) { %>
    "build:modernizr": "modernizr -c modernizr.json -d app/scripts/modernizr.js",
    "serve:test": "npm run build:modernizr && cross-env NODE_ENV=test gulp serve",
    "serve:dist": "npm run build:modernizr && cross-env NODE_ENV=production gulp serve",
    "start": "npm run build:modernizr && gulp serve",
    <% } %>
    <% if (!includeModernizr) { %>
    "serve:test": "cross-env NODE_ENV=test gulp serve",
    "serve:dist": "cross-env NODE_ENV=production gulp serve",
    "start": "gulp serve",
    <% } %>
    "build": "cross-env NODE_ENV=production gulp",
    "test": "npm run serve:test",
    "tasks": "gulp --tasks"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
      <% if (includeJQuery || includeBootstrap) { %>,
      "jquery": true
      <% } %>
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  }
}
